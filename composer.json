{
    "name": "larsnieuwenhuizen/e-utilities",
    "description": "Php library for communication with the NCBI E-Utilities library",
    "type": "library",
    "require": {
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.2",
        "phpunit/phpunit": "^7.1",
        "codeclimate/php-test-reporter": "^0.4.4"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Lars Nieuwenhuizen",
            "email": "l.nieuwenhuizen@developers.nl"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "LarsNieuwenhuizen\\EUtilities\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LarsNieuwenhuizen\\EUtilities\\Tests\\": "tests/"
        }
    }
}
